{
    "name": "tnlake/lib",
    "version": "0.5.5",
    "description": "Utilities for Laravel 12 (config publish only, no routes).",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/tnlake/tnlake-lib",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Tnlake\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tnlake\\LibServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    }
}
